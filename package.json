        "prepush": "yarn format && yarn fix && yarn build:prod && yarn test:prod && yarn e2e",
        "start": "yarn ng serve --host 0.0.0.0",
        "test": "yarn ng test",
        "test:prod": "yarn ng test --watch=false",
        "update": "ng update @nrwl/workspace",
        "update:check": "yarn ng update",
        "workspace-schematic": "yarn nx workspace-schematic"
    },
