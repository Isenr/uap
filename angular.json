{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "defaultProject": "root-core",
    "newProjectRoot": "",
    "projects": {
        "auth": {
            "root": "libs/auth",
            "sourceRoot": "libs/auth/src",
            "projectType": "library",
            "prefix": "uap",
            "architect": {
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "libs/auth/src/test.ts",
                        "tsConfig": "libs/auth/tsconfig.spec.json",
                        "karmaConfig": "libs/auth/karma.conf.ts"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": ["libs/auth/tsconfig.lib.json", "libs/auth/tsconfig.spec.json"],
                        "exclude": ["**/node_modules/**"]
                    }
                }
            },
            "schematics": {
                "@nrwl/schematics:component": {
                    "styleext": "scss"
                }
            }
        },
        "backend": {
            "root": "libs/backend",
            "sourceRoot": "libs/backend/src",
            "projectType": "library",
            "prefix": "uap",
            "architect": {
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "libs/backend/src/test.ts",
                        "tsConfig": "libs/backend/tsconfig.spec.json",
                        "karmaConfig": "libs/backend/karma.conf.ts"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "libs/backend/tsconfig.lib.json",
                            "libs/backend/tsconfig.spec.json"
                        ],
                        "exclude": ["**/node_modules/**"]
                    }
                }
            },
            "schematics": {
                "@nrwl/schematics:component": {
                    "styleext": "scss"
                }
            }
        },
        "products-components": {
            "prefix": "uap",
            "projectType": "library",
            "root": "libs/products/components",
            "sourceRoot": "libs/products/components/src",
            "architect": {
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "libs/products/components/src/test.ts",
                        "karmaConfig": "libs/products/components/karma.conf.ts",
                        "tsConfig": "libs/products/components/tsconfig.spec.json"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "exclude": ["**/node_modules/**"],
                        "tsConfig": [
                            "libs/products/components/tsconfig.lib.json",
                            "libs/products/components/tsconfig.spec.json"
                        ]
                    }
                }
            }
        },
        "products-models": {
            "root": "libs/products/models",
            "sourceRoot": "libs/products/models/src",
            "projectType": "library",
            "prefix": "uap",
            "architect": {
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "libs/products/models/src/test.ts",
                        "karmaConfig": "libs/products/models/karma.conf.ts",
                        "tsConfig": "libs/products/models/tsconfig.spec.json"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "exclude": ["**/node_modules/**"],
                        "tsConfig": [
                            "libs/products/models/tsconfig.lib.json",
                            "libs/products/models/tsconfig.spec.json"
                        ]
                    }
                }
            }
        },
        "products-state": {
            "root": "libs/products/state",
            "sourceRoot": "libs/products/state/src",
            "projectType": "library",
            "prefix": "uap",
            "architect": {
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "libs/products/state/src/test.ts",
                        "tsConfig": "libs/products/state/tsconfig.spec.json",
                        "karmaConfig": "libs/products/state/karma.conf.ts"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "libs/products/state/tsconfig.lib.json",
                            "libs/products/state/tsconfig.spec.json"
                        ],
                        "exclude": ["**/node_modules/**"]
                    }
                }
            },
            "schematics": {
                "@nrwl/schematics:component": {
                    "styleext": "scss"
                }
            }
        },
        "root-core": {
            "prefix": "uap",
            "projectType": "application",
            "root": "apps/root-core/",
            "sourceRoot": "apps/root-core/src",
            "schematics": {
                "@schematics/angular:component": {
                    "styleext": "scss"
                }
            },
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "index": "apps/root-core/src/index.html",
                        "main": "apps/root-core/src/main.ts",
                        "outputPath": "dist/apps/root-core",
                        "polyfills": "apps/root-core/src/polyfills.ts",
                        "tsConfig": "apps/root-core/tsconfig.app.json",
                        "assets": [
                            "apps/root-core/src/favicon.ico",
                            "apps/root-core/src/assets",
                            "apps/root-core/src/manifest.json"
                        ],
                        "scripts": [],
                        "styles": ["apps/root-core/src/styles.scss"]
                    },
                    "configurations": {
                        "production": {
                            "aot": true,
                            "buildOptimizer": true,
                            "extractCss": true,
                            "extractLicenses": true,
                            "namedChunks": false,
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": true,
                            "vendorChunk": false,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                }
                            ],
                            "fileReplacements": [
                                {
                                    "replace": "apps/root-core/src/environments/environment.ts",
                                    "with": "apps/root-core/src/environments/environment.prod.ts"
                                }
                            ],
                            "serviceWorker": true
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "root-core:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "root-core:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "root-core:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "apps/root-core/src/test.ts",
                        "karmaConfig": "apps/root-core/karma.conf.ts",
                        "polyfills": "apps/root-core/src/polyfills.ts",
                        "tsConfig": "apps/root-core/tsconfig.spec.json",
                        "assets": [
                            "apps/root-core/src/favicon.ico",
                            "apps/root-core/src/assets",
                            "apps/root-core/src/manifest.json"
                        ],
                        "styles": ["apps/root-core/src/styles.scss"],
                        "scripts": []
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "apps/root-core/tsconfig.app.json",
                            "apps/root-core/tsconfig.spec.json"
                        ],
                        "exclude": ["**/node_modules/**"]
                    }
                }
            }
        },
        "root-core-e2e": {
            "root": "apps/root-core-e2e",
            "projectType": "application",
            "prefix": "",
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "devServerTarget": "root-core:serve",
                        "protractorConfig": "apps/root-core-e2e/protractor.conf.ts"
                    },
                    "configurations": {
                        "production": {
                            "devServerTarget": "root-core:serve:production"
                        }
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": "apps/root-core-e2e/tsconfig.e2e.json",
                        "exclude": ["**/node_modules/**"]
                    }
                }
            }
        },
        "state": {
            "prefix": "uap",
            "projectType": "library",
            "root": "libs/state",
            "sourceRoot": "libs/state/src",
            "architect": {
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "libs/state/src/test.ts",
                        "karmaConfig": "libs/state/karma.conf.ts",
                        "tsConfig": "libs/state/tsconfig.spec.json"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "exclude": ["**/node_modules/**"],
                        "tsConfig": [
                            "libs/state/tsconfig.lib.json",
                            "libs/state/tsconfig.spec.json"
                        ]
                    }
                }
            }
        },
        "utils": {
            "prefix": "uap",
            "projectType": "library",
            "root": "libs/utils",
            "sourceRoot": "libs/utils/src",
            "architect": {
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "libs/utils/src/test.ts",
                        "karmaConfig": "libs/utils/karma.conf.ts",
                        "tsConfig": "libs/utils/tsconfig.spec.json"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "exclude": ["**/node_modules/**"],
                        "tsConfig": [
                            "libs/utils/tsconfig.lib.json",
                            "libs/utils/tsconfig.spec.json"
                        ]
                    }
                }
            }
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/angular",
        "packageManager": "yarn",
        "warnings": {
            "typescriptMismatch": false,
            "versionMismatch": false
        }
    }
}
